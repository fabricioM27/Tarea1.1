<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Tarea1._1.PageResult"
             Title="PageResult">
    <VerticalStackLayout>
        <Label Text="El resultado es: " FontSize="Title" Padding="30,50,30,10" HorizontalOptions="Center" />
        <Label Text="{Binding resultado}" HorizontalOptions="Center" FontSize="15" Margin="30,30,30,0"/>
    </VerticalStackLayout>
</ContentPage>